<!DOCTYPE html>
<html>
<head>
  <title>FreePee Club</title>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
  <script type="text/javascript"
  src="https://maps.googleapis.com/maps/api/js?key=<%=Figaro.env.google_map_key%>">
  </script>
  <%= include_gon %>
</head>
<body class="home">

  <div class="row">
    <div class="col-4-12 colCentered textCenter">
      <%= yield %>
    </div>
  </div>


</body>
</html>
